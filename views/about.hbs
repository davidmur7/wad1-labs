{{> menu id="about"}}

<section class="ui left aligned middle aligned segment">
<div class="ui centered card">
  <div class="image">
    <img src="{{info.image}}">
  </div>
  <div class="content">
    <a class="header">{{info.fullname}} </a>
    <div class="meta">
      <span class="date">Joined {{info.joined}}</span>
    </div>
    <div class="description">
        {{info.name}} is a {{info.job}} living in {{info.city}}.
    </div>
  </div>
  <div class="extra content">
    <a>
      <i class="user icon"></i>
        35 contacts
    </a>
  </div>
</div>
</section>
